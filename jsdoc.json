{
    "tags": {
      "allowUnknownTags": true
    },
    "source": {
      "include": [
        "src"
      ],
      "exclude": [
        "node_modules"
      ],
      "includePattern": ".+\\.js(doc)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": false
    },
    "opts": {
      "destination": "./docs",
      "recurse": true
    }
  }