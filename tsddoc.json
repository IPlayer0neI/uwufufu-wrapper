{
    "tags": {
      "allowUnknownTags": true
    },
    "source": {
      "include": ["src"],
      "exclude": ["node_modules"],
      "includePattern": ".+\\.js(doc)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": false
    },
    "opts": {
      "destination": "./dist",
      "recurse": true,
      "template": "./node_modules/tsd-jsdoc/dist"
    }
  }